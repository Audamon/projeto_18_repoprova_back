{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules":{
        "import/extensions": [
        "error",
        {
        "js": "ignorePackages"
        }
    ],
    "import/no-unresolved": 0,
    "import/prefer-default-export": 0,
    "no-nested-ternary":"off",
    "max-len":"off",
    "no-console": 0,
    "consistent-return":"off"
    }
}
